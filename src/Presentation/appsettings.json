{
  "Azure": {
    "BlobStorage": {
      "ContainerName": "files"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning"
      }
    },
    "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId", "WithSpan"],
    "WriteTo": [{ "Name": "Console" }, { "Name": "OpenTelemetry" }],
    "Properties": {
      "Application": "mirai-api",
      "Environment": "Production"
    }
  },
  "Authentication": {
    "Audience": "account",
    "ValidIssuer": "",
    "MetadataAddress": "",
    "RequireHttpsMetadata": false
  },
  "Keycloak": {
    "AdminUrl": "",
    "TokenUrl": "",
    "AdminClientId": "mirai-admin-client",
    "AdminClientSecret": "",
    "AuthClientId": "mirai-auth-client",
    "AuthClientSecret": ""
  },
  "Cors": {
    "AllowedOrigins": ["https://miraihq.com"]
  }
}
